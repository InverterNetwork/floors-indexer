{
  "name": "envio-indexer",
  "version": "0.1.0",
  "scripts": {
    "clean": "tsc --clean",
    "build": "tsc --build",
    "watch": "tsc --watch",
    "lint": "eslint . --ext .ts --fix",
    "mocha": "ts-mocha test/**/*.ts",
    "codegen": "envio codegen",
    "dev": "eval \"$(scripts/select_rpc.sh)\" && LOG_LEVEL=trace LOG_STRATEGY=console-pretty TUI_OFF=true envio dev",
    "start": "envio start",
    "test": "pnpm mocha",
    "db-up": "cd generated && pnpm db-up && cd -",
    "db-down": "cd generated && pnpm db-down && cd -",
    "db-setup": "cd generated && pnpm db-setup && cd -"
  },
  "devDependencies": {
    "@types/chai": "^4.3.20",
    "@types/mocha": "10.0.6",
    "@types/node": "20.8.8",
    "chai": "4.3.10",
    "mocha": "10.2.0",
    "ts-mocha": "^10.1.0",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "envio": "2.32.3",
    "viem": "^2.39.0"
  },
  "optionalDependencies": {
    "generated": "link:generated"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
